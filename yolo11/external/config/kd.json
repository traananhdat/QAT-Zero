{
    "device": "cuda",
    "dataset_manifest": "coco.yaml",
    "batch_size": 16,
    "fraction": 0.1,
    "generated_weights_path": "./runs/even_setting/weights",
    "calibration_size": 10000,
    "model": "yolo11s.pt",
    "model_quantize_mode": {
        "kind": "quantize_sym",
        "weight_bits": 8,
        "activation_bits": 8
    },
    "kd_method": {
        "teacher_weight": "yolo11s.pt",
        "kd_module": "cnnbn",
        "original_loss_weight": 0.01,
        "kd_loss": {
            "kind": "kl",
            "tau": 4.0
        },
        "kd_loss_weight": 0.1,
        "mse_loss_weight": 1.0
    },
    "hyps": {
        "optimizer_name": "Adam",
        "lr0": 1e-5,
        "lrf": 0.01,
        "momentum": 0.937,
        "weight_decay": 0.0005,
        "box": 7.5,
        "cls": 0.5,
        "dfl": 1.5
    },
    "end_epochs": 100,
    "patience": 10
}

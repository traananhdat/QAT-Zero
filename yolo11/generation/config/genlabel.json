{
  "project": "runs/DistillOneBox",
  "name": "exp",
  "device": "cuda",
  "teacher_weights": "yolo11s.pt",
  "relabel_weights": "yolo11s.pt",
  "dataset_configs": {
    "kind": "real",
    "data": "data/cocoonebox.yaml",
    "batch_size": 512,
    "workers": 8
  },
  "img_size": 160,
  "calibration_size": 2048,
  "da_configs": {
    "do_jitter": false,
    "do_flip": false,
    "rand_brightness": false,
    "rand_contrast": false,
    "random_erase": false
  },
  "iterations": 3000,
  "hyp": {
    "lr": 0.2,
    "r_feature": 0.05,
    "tv_l1": 0,
    "tv_l2": 0.002,
    "main_loss_multiplier": 0.5,
    "first_bn_coef": 5.0
  },
  "box_sampler_config": {
    "box_sampler": true,
    "box_sampler_warmup": 800,
    "box_sampler_conf": 0.2,
    "box_sampler_overlap_iou": 0.35,
    "box_sampler_minarea": 0.01,
    "box_sampler_maxarea": 0.85,
    "box_sampler_earlyexit": 2800
  },
  "save_every": 100,
  "save_images": false,
  "skip_generated": false
}

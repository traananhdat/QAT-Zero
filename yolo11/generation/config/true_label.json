{
  "project": "runs/Distill",
  "name": "exp",
  "device": "cuda",
  "teacher_weights": "yolo11s.pt",
  "relabel_weights": "yolo11s.pt",
  "dataset_configs": {
    "kind": "real",
    "data": "coco.yaml",
    "batch_size": 32,
    "workers": 8
  },
  "img_size": 640,
  "calibration_size": 2048,
  "iterations": 3000,
  "hyp": {
    "lr": 0.2,
    "r_feature": 0.001,
    "tv_l1": 0,
    "tv_l2": 0.00005,
    "main_loss_multiplier": 0.001,
    "first_bn_coef": 1.0
  },
  "box_sampler_config": {
    "box_sampler": false,
    "box_sampler_warmup": 0,
    "box_sampler_conf": 0.5,
    "box_sampler_overlap_iou": 0.2,
    "box_sampler_minarea": 0.0,
    "box_sampler_maxarea": 1.0,
    "box_sampler_earlyexit": 1000000
  },
  "save_every": 100,
  "save_images": false,
  "skip_generated": false
}
